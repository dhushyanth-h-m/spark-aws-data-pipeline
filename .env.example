# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_REGION=us-east-1

# Project Configuration
PROJECT_NAME=nyc-taxi-pipeline
ENVIRONMENT=dev

# EMR Configuration
KEEP_ALIVE=false
INSTANCE_COUNT=3
MASTER_INSTANCE_TYPE=m5.xlarge
SLAVE_INSTANCE_TYPE=m5.xlarge
USE_SPOT=true

# Optional: EC2 Key Pair for SSH access to EMR cluster
# EC2_KEY_NAME=your_key_pair_name 